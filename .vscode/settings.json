{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "tsconfig.json": "tsconfig.*.json, env.d.ts",
    "vite.config.*": "jsconfig*, vitest.config.*, cypress.config.*, playwright.config.*",
    "package.json": "package-lock.json, pnpm*, .yarnrc*, yarn*, .eslint*, eslint*, .prettier*, prettier*, .editorconfig, .tool-versions, .jscpd.frontend.json, .reviewdog.yml",
    "Jenkinsfile": "Jenkinsfile, Jenkins.properties",
    "Dockerfile": "Dockerfile, docker-compose*",
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
}
